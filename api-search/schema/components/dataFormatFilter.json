{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Data Format Filter",
  "description": "Filter results by data format name(s) (e.g. - NETCDF).",
  "type": "object",
  "properties": {
    "type": {
      "description": "Filter type.",
      "enum": [
        "dataFormat"
      ]
    },
    "values": {
      "description": "List of data formats.",
      "type": "array",
      "items": {
        "description": "Data Format",
        "type": "string"
      }
    }
  },
  "required": [
    "type",
    "values"
  ],
  "additionalProperties": false
}
