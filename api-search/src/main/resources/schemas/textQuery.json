{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Text query to search with that is valid so long as it does not have a leading wildcard ('*' or '?')",
  "properties": {
    "type": {
      "enum": [
        "queryText"
      ]
    },
    "value": {
      "type": "string",
      "pattern": "^(?!\\s*[\\*\\?]+).*"
    }
  },
  "required": [
    "type",
    "value"
  ],
  "additionalProperties": false
}
