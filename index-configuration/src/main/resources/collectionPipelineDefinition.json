{
  "description": "[RELEASE-DATE] Transforms and reduces fields from staging_collection to search_collection",
  "processors": [
    {
      "foreach": {
        "field": "keywords",
        "processor": {
          "append": {
            "field": "keywordValues",
            "value": "{{ keywords.values }}"
          }
        }
      },
      "foreach": {
        "field": "contacts",
        "processor": {
          "append": {
            "field": "individualNames",
            "value": "{{ contacts.individualName }}"
          }
        }
      },
      "foreach": {
        "field": "contacts",
        "processor": {
          "append": {
            "field": "organizationNames",
            "value": "{{ contacts.organizationName }}"
          }
        }
      },
      "foreach": {
        "field": "creators",
        "processor": {
          "append": {
            "field": "individualNames",
            "value": "{{ creators.individualName }}"
          }
        }
      },
      "foreach": {
        "field": "creators",
        "processor": {
          "append": {
            "field": "organizationNames",
            "value": "{{ creators.organizationName }}"
          }
        }
      },
      "foreach": {
        "field": "publishers",
        "processor": {
          "append": {
            "field": "individualNames",
            "value": "{{ publishers.individualName }}"
          }
        }
      },
      "foreach": {
        "field": "publishers",
        "processor": {
          "append": {
            "field": "organizationNames",
            "value": "{{ publishers.organizationName }}"
          }
        }
      },
      "set": {
        "field": "beginDate",
        "value": "{{ temporalBounding.beginDate }}"
      },
      "set": {
        "field": "beginYear",
        "value": "{{ temporalBounding.beginYear }}"
      },
      "set": {
        "field": "endDate",
        "value": "{{ temporalBounding.endDate }}"
      },
      "set": {
        "field": "endYear",
        "value": "{{ temporalBounding.endYear }}"
      },
      "remove": {
        "field": [
          "parentIdentifier",
          "purpose",
          "status",
          "credit",
          "hierarchyLevelName",
          "alternateTitle",
          "keywords",
          "accessionValues",
          "topicCategories",
          "temporalBounding",
          "acquisitionInstruments",
          "acquisitionOperations",
          "acquisitionPlatforms",
          "contacts",
          "creators",
          "publishers",
          "thumbnailDescription",
          "creationDate",
          "revisionDate",
          "publicationDate",
          "dsmmAccessibility",
          "dsmmDataIntegrity",
          "dsmmDataQualityAssessment",
          "dsmmDataQualityAssurance",
          "dsmmDataQualityControlMonitoring",
          "dsmmPreservability",
          "dsmmProductionSustainability",
          "dsmmTransparencyTraceability",
          "dsmmUsability",
          "updateFrequency",
          "presentationForm",
          "services"
        ]
      },
      "rename": {
        "field": "keywordValues",
        "target_field": "keywords"
      }
    }
  ]
}